{% block content %}
  <div class="table-responsive">
    <table class="table table-striped table-bordered table-dark">
      <thead class="thead-dark">
      <tr>
        {% for col in component.data.cols %}
          <td>{{ col }}</td>
        {% endfor %}
      </tr>
      </thead>
      <tbody>
      {% for key,row in component.data.rows %}
        <tr>
          {% for i in 1..component.iterations %}
            {{ cycle('<td>'~row[i]~'</td>', i)|raw }}
          {% endfor %}
        </tr>
      {% endfor %}
      </tbody>
    </table>
  </div>
{% endblock %}
