{% extends layout %}

{% block content %}
<div class="table-responsive">
  <table class="table table-striped table-bordered">
    <thead>
      <tr>
        {% for head in headers %}
          <th>{{ head|capitalize }}</th>
        {% endfor %}
      </tr>
    </thead>
    <tbody>
    {% for item in items %}
      <tr>
        {% for value in item  %}
          <td>{{ value|capitalize }}</td>
        {% endfor %}
      </tr>
    {% endfor %}
    </tbody>
  </table>
</div>
{% endblock %}
